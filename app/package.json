{
  "name": "temis",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "lint-staged && npm test",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test-ci": "npm run lint && npm run test",
    "lint": "npm run lint:js -- **/*.{js,jsx}",
    "lint:js": "prettier --config ../.prettierrc --write",
    "lint:json": "jsonlint"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "16.0.0",
    "react-native": "0.50.1",
    "react-native-facebook-login": "^1.6.0",
    "react-native-i18n": "^2.0.9",
    "react-native-navigation": "^1.1.262",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0",
    "redux-reset": "^0.3.0",
    "reduxsauce": "^0.7.0"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "codecov": "^3.0.0",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "21.2.1",
    "jsonlint": "^1.6.2",
    "lint-staged": "^4.3.0",
    "prettier": "^1.8.2",
    "react-test-renderer": "16.0.0",
    "reactotron-react-native": "^1.12.3",
    "reactotron-redux": "^1.12.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "gitDir": "../",
    "linters": {
      "*.{js,jsx}": [
        "npm run lint:js -- --write",
        "git add"
      ],
      "*.json": [
        "npm run lint:json",
        "git add"
      ]
    }
  }
}
